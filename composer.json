{
  "name": "chargefield/laravel-widget",
  "description": "Widget based approach for structuring portions of your views.",
  "license": "MIT",
  "authors": [
    {
      "name": "Clayton D'Mello",
      "email": "clayton@chargefield.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Chargefield\\LaravelWidget\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chargefield\\LaravelWidget\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.2",
    "matt-allan/laravel-code-style": "^0.4.0",
    "mockery/mockery": "^1.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chargefield\\LaravelWidget\\WidgetServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
